{
  "name": "strategic-landing",
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "astro",
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        }
      ]
    }
  ],
  "github": {
    "enabled": true,
    "silent": true
  },
  "alias": ["strategic-landing.vercel.app"],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
} 